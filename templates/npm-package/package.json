    "chai": "^6.0.1",
    "eslint": "^8.56.0",
    "eslint-config-nstarter": "^4.0.1",
    "mocha": "^11.7.2",
    "nstarter-core": "^1.3.1",
    "nstarter-tsconfig": "0.5.0",
    "nyc": "^17.1.0",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.2"
  },
  "engines": {
    "node": ">=22.19.0"
  }
